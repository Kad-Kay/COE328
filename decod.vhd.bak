LIBRARY ieee;
USE ieee.std_logic_1164.all;

ENTITY decod IS
	PORT (w		:IN STD_LOGIC_VECTOR(2 DOWNTO 0);
			En		:IN STD_LOGIC;
			y		:OUT STD_LOGIC_VECTOR(0 TO 7));
END decod;

ARCHITECTURE Behavior OF decod IS

BEGIN

	WITH En SELECT
		y <= "00000001" WHEN "000",
			  "00000010" WHEN "001",
			  "00000100" WHEN "010",
			  "00001000" WHEN "011",
			  "00010000" WHEN "100",
			  "00100000" WHEN "101",
			  "01000000" WHEN "110",
			  "10000000" WHEN "111",
			  "00000000" WHEN OTHERS;
END Behavior;